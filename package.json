{
  "name": "animated-time",
  "version": "0.0.1",
  "description": "A simple Typescript bolierplate with boilerplate",
  "author": "tizee",
  "license": "MIT",
  "scripts": {
    "ts-check": "tsc --noEmit",
    "ts-check:watch": "tsc --noEmit --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "eslint src",
    "format": "prettier src/**/*.{js,ts,jsx,tsx} --write",
    "dev": "cross-env webpack-dev-server --config webpack.dev.js --open",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@commitlint/config-angular": "^9.1.1",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "commitlint": "^9.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "fibers": "^5.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.5",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.6",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "highlight.js": "^10.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "redux": "^4.0.5",
    "tz-codecard": "^0.0.1"
  }
}
